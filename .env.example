# ConsoleSensei Cloud - Frontend Environment Variables

# API Configuration
VITE_API_URL=https://api.consolesensei.com/v1

# Use mock data (set to 'false' to use real backend)
VITE_USE_MOCK_DATA=true

# Supabase Configuration (optional)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# AWS Cognito Configuration (optional - for future backend integration)
VITE_COGNITO_REGION=us-east-1
VITE_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_COGNITO_DOMAIN=your-domain.auth.us-east-1.amazoncognito.com
